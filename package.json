{
  "name": "coderbase-starter",
  "version": "0.0.0",
  "scripts": {
    "cypress-open": "cypress open",
    "cy:run": "cypress run",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --prod",
    "test": "npx cypress open",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "generate": "node api/generate.js > api/db.json",
    "json-server": "npm run generate && cd api && node server.js",
    "ci:json-server": "npm run generate && cd api && nohup node server.js &",
    "compodoc": "compodoc -s -o -p src/tsconfig.app.json --language fr-FR --hideGenerator",
    "compodoc-ci": "compodoc -p src/tsconfig.app.json --silent --language fr-FR --hideGenerator",
    "lint-fix": "tslint --fix ./src/**/*.ts -t verbose",
    "ci:start-server": "angular-http-server --path ./dist/coderbase-starter -p 4200",
    "ci:cy-run": "start-server-and-test ci:start-server http://localhost:4200 cy:run",
    "cy:verify": "cypress verify",
    "cypress-multi" : "cypress ci --spec 'cypress/integration/tests/$CI_BUILD_NAME.js'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "@briebug/cypress-schematic": "^2.0.0",
    "@coderbase/ui": "0.0.6",
    "@fortawesome/angular-fontawesome": "^0.4.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/free-solid-svg-icons": "^5.8.2",
    "@ng-bootstrap/ng-bootstrap": "^4.2.0",
    "@ng-bootstrap/schematics": "^2.0.0-alpha.1",
    "bootstrap": "^4.3.1",
    "core-js": "^3.1.3",
    "ngx-cdmon": "^0.1.2",
    "rxjs": "~6.5.2",
    "tslib": "^1.9.3",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular-devkit/build-ng-packagr": "~0.800.0",
    "@angular/cli": "~8.0.0",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@cypress/webpack-preprocessor": "4.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~12.0.3",
    "angular-http-server": "^1.9.0",
    "codelyzer": "^5.0.1",
    "cypress": "3.3.1",
    "faker": "^4.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "json-server": "^0.15.0",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^5.2.0",
    "start-server-and-test": "^1.9.1",
    "ts-loader": "6.0.2",
    "ts-node": "~8.2.0",
    "tsickle": ">=0.34.0",
    "tslint": "~5.11.0",
    "typescript": "~3.4.5"
  },
  "config": {
    "multi-cypress": {
      "specs": "cypress/integration/tests/*_spec.js",
      "image": "registry.gitlab.com/coderbaseit/crm-grenoble-mai/coderbase-test:12.1.0"
    }
  }
}
